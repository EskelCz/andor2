{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~assets/*": [
        "src/assets/*"
      ],
      "~components/*": [
        "src/components/*"
      ],
      "~db/*": [
        "src/db/*"
      ],
      "~lib/*": [
        "src/lib/*"
      ],
      "~pages/*": [
        "src/pages/*"
      ],
      "~/*": [
        "src/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@db/*": [
        "src/db/*"
      ],
      "@lib/*": [
        "src/lib/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@/*": [
        "src/*"
      ],
      "@store/*": [
        "src/store/*"
      ],
      "@stores/*": [
        "src/stores/*"
      ],
      "@routes/*": [
        "src/routes/*"
      ]
    }
  }
}